{
  "name": "classnames-creator",
  "version": "1.0.6",
  "main": "dist-node/index.js",
  "browser": "dist-browser/index.js",
  "types": "src/index.d.ts",
  "license": "MIT",
  "description": "Concats CSS classnames, based on conditions",
  "type": "module",
  "scripts": {
    "build-browser": "tsc src/index.ts --outDir dist-browser --module ES6",
    "build-node": "tsc src/index.ts --outDir dist-node --module nodenext",
    "build-type": "tsc src/index.ts -declaration --allowJs --emitDeclarationOnly",
    "build": "npm run build-browser & npm run build-node & npm run build-type"
  },
  "dependencies": {
    "@types/node": "^18.0.1",
    "typescript": "^4.7.4"
  },
  "author": {
