    "email": "fentos.dev@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/victor-lillo/classnames-creator.git"
  },
  "homepage": "https://github.com/victor-lillo/classnames-creator",
  "keywords": [
    "nextjs",
    "react",
    "reactjs",
    "conditionally",
    "class-names",
    "classnames",
    "concat",
    "concatenate"
  ]
}
